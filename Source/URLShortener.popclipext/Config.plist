<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>After</key>
			<string>paste-result</string>
			<key>Image File</key>
			<string>urlshortener.png</string>
			<key>Regular Expression</key>
			<string>^.{4,}</string>
			<key>Script Interpreter</key>
			<string>/usr/bin/python</string>
			<key>Shell Script File</key>
			<string>urlshortener.py</string>
			<key>Title</key>
            <string>URL Shortener</string>
            <key>Regular Expression</key>
            <string>/^(https?:\/\/)?([\da-z\.-]+)\.([a-z\.]{2,6})([\/\w \.-]*)*\/?$/</string>
		</dict>
	</array>
	<key>Credits</key>
	<array>
		<dict>
			<key>Link</key>
			<string>http://zlz.im/</string>
			<key>Name</key>
			<string>hzlzh</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Turns a long URL into a much shorter one. Several service are available.</string>
	<key>Extension Identifier</key>
	<string>com.hzlzh.popclip.extension.urlshortener</string>
	<key>Extension Name</key>
	<string>URL Shortener</string>
	<key>Version</key>
	<integer>2</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Option Identifier</key>
			<string>domain</string>
			<key>Option Label</key>
			<dict>
				<key>en</key>
				<string>Choose URL Prefix</string>
			</dict>
			<key>Option Type</key>
			<string>multiple</string>
			<key>Option Values</key>
			<array>
				<string>bit.ly</string>
				<string>goo.gl</string>
				<string>t.cn</string>
				<string>j.mp</string>
				<string>is.gd</string>
				<string>v.gd</string>
				<string>tiny.cc</string>
			</array>
		</dict>
	</array>
	<key>Options Title</key>
	<dict>
		<key>en</key>
		<string>Shorter Services List</string>
	</dict>
</dict>
</plist>
